  <!-- flat_list+flat_fwd_list, paged storage, dynamic page size -->
  <Type Name="c4::flat_fwd_list&lt;*,*,c4::raw_paged&lt;*,0,*,*,*&gt;&gt;">
    <AlternativeType Name="c4::flat_fwd_list&lt;*,*,c4::stg::raw_paged_rt&lt; *, * &gt;&gt;"/>
    <AlternativeType Name="c4::flat_fwd_list&lt;*,*,c4::default_list_storage&gt;"/>
    <AlternativeType Name="c4::flat_list&lt;*,*,c4::raw_paged&lt;*,0,*,*,*&gt;&gt;"/>
    <AlternativeType Name="c4::flat_list&lt;*,*,c4::stg::raw_paged_rt&lt; *, * &gt;&gt;"/>
    <AlternativeType Name="c4::flat_list&lt;*,*,c4::default_list_storage&gt;"/>
    <!-- -->
    <DisplayString>{{sz={m_size}}}</DisplayString>
    <Expand>
      <Item Name="[size]">m_size</Item>
      <Item Name="[head]" Condition="m_head == npos">npos</Item>
      <Item Name="[head]" Condition="m_head != npos">m_head</Item>
      <Item Name="[tail]" Condition="m_tail == npos">npos</Item>
      <Item Name="[tail]" Condition="m_tail != npos">m_tail</Item>
      <Item Name="[m_elms]">m_elms</Item>
      <!-- https://msdn.microsoft.com/en-us/library/jj620914.aspx -->
      <CustomListItems MaxItemsPerView="500" ExcludeView="Test">
        <Variable Name="cursor" InitialValue="m_head" />
        <Size>m_size</Size>
        <Loop>
          <If Condition="cursor != npos">
            <Item>m_elms.m_pages[cursor >> m_elm.m_page_lsb][cursor &amp; m_elm.m_id_mask].elm</Item>
            <Exec>cursor = m_elms.m_pages[cursor >> m_next.m_page_lsb][cursor &amp; m_next.m_id_mask].next</Exec>
          </If>
        </Loop>
      </CustomListItems>
    </Expand>
  </Type>
  <!-- flat_list+flat_fwd_list, paged storage, fixed page size -->
