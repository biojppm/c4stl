      <Item Name="[page size]">$T2</Item>
      <Item Name="[num pages]">m_num_pages</Item>
      <Synthetic Name="[page 0]" Condition="m_num_pages > 0"><Expand><ArrayItems Condition="m_num_pages > 0"><Size>$T2</Size><ValuePointer>m_pages[0]</ValuePointer></ArrayItems></Expand></Synthetic>
      <Synthetic Name="[page 1]" Condition="m_num_pages > 1"><Expand><ArrayItems Condition="m_num_pages > 1"><Size>$T2</Size><ValuePointer>m_pages[1]</ValuePointer></ArrayItems></Expand></Synthetic>
      <Synthetic Name="[page 2]" Condition="m_num_pages > 2"><Expand><ArrayItems Condition="m_num_pages > 2"><Size>$T2</Size><ValuePointer>m_pages[2]</ValuePointer></ArrayItems></Expand></Synthetic>
      <Synthetic Name="[page 3]" Condition="m_num_pages > 3"><Expand><ArrayItems Condition="m_num_pages > 3"><Size>$T2</Size><ValuePointer>m_pages[3]</ValuePointer></ArrayItems></Expand></Synthetic>
      <Synthetic Name="[page 4]" Condition="m_num_pages > 4"><Expand><ArrayItems Condition="m_num_pages > 4"><Size>$T2</Size><ValuePointer>m_pages[4]</ValuePointer></ArrayItems></Expand></Synthetic>
      <Synthetic Name="[page 5]" Condition="m_num_pages > 5"><Expand><ArrayItems Condition="m_num_pages > 5"><Size>$T2</Size><ValuePointer>m_pages[5]</ValuePointer></ArrayItems></Expand></Synthetic>
